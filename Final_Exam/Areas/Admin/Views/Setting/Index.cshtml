@model List<Setting>
@{
    int count = 0;
}
<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">


            <div class="table-responsive">
                <h4 class="card-title">Setting</h4>

                <table class="table">
                    <thead>
                        <tr>
                            <th>No</th>
                            <th>Key</th>
                            <th>Value</th>
                            <th>Settings</th>
                        </tr>
                    </thead>
                    <tbody>

                        @foreach (Setting setting in Model)
                        {
                            count++;
                            <tr>
                                <td>@count</td>
                                <td>@setting.Key</td>
                                <td>@setting.Value</td>
                                <td>
                                    <a class="btn btn-inverse-warning" asp-action="Update" asp-controller="Setting" asp-route-id="@setting.Id">Update</a>
                                    
                                </td>
                            </tr>

                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>